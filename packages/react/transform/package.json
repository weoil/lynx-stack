{
  "name": "@lynx-js/react-transform",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./main.js"
      },
      "require": {
        "types": "./cjs/main.d.cts",
        "default": "./cjs/main.cjs"
      }
    }
  },
  "main": "./main.js",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "main.js",
    "main.cjs",
    "cjs",
    "dist",
    "!dist/*.node"
  ],
  "scripts": {
    "bench": "vitest bench",
    "build:debug": "napi build --platform --js index.cjs",
    "build:wasm": "node ./scripts/build_wasm.js",
    "test": "vitest --coverage",
    "test:cargo": "cargo test"
  },
  "devDependencies": {
    "@emnapi/core": "1.3.1",
    "@emnapi/runtime": "1.3.1",
    "@napi-rs/cli": "2.18.4",
    "esbuild": "^0.25.4",
    "tiny-glob": "0.2.9"
  }
}
