{
  "name": "@lynx-js/rspeedy",
  "version": "0.8.4",
  "description": "A webpack/rspack-based frontend toolchain for Lynx",
  "keywords": [
    "webpack",
    "rspack",
    "Lynx",
    "ReactLynx"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lynx-family/lynx-stack.git",
    "directory": "packages/rspeedy/core"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Qingyu Wang",
    "email": "colinwang.0616@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./register": {
      "types": "./register/index.d.ts",
      "import": "./register/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "bin": {
    "rspeedy": "./bin/rspeedy.js"
  },
  "files": [
    "bin",
    "client",
    "lib",
    "!lib/**/*.js.map",
    "register",
    "client.d.ts",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --verbose",
    "test": "pnpm -w run test --project rspeedy",
    "test:type": "vitest --typecheck.only"
  },
  "dependencies": {
    "@lynx-js/chunk-loading-webpack-plugin": "workspace:^",
    "@lynx-js/webpack-dev-transport": "workspace:^",
    "@lynx-js/websocket": "workspace:^",
    "@microsoft/api-extractor": "catalog:",
    "@rsbuild/core": "catalog:rsbuild",
    "@rsbuild/plugin-css-minimizer": "1.0.2",
    "@rsdoctor/rspack-plugin": "1.0.0-rc.0",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "exit-hook": "^4.0.0",
    "ipaddr.js": "^2.2.0",
    "javascript-stringify": "^2.1.0",
    "picocolors": "^1.1.1",
    "tiny-invariant": "^1.3.3",
    "ts-blank-space": "^0.6.1",
    "typia": "7.6.4"
  },
  "devDependencies": {
    "@lynx-js/vitest-setup": "workspace:*",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rsbuild/webpack": "catalog:rsbuild",
    "eventemitter3": "^5.0.1",
    "type-fest": "^4.37.0",
    "vitest": "^3.0.8",
    "webpack": "^5.98.0"
  },
  "engines": {
    "node": ">=18"
  },
  "tsdocMetadata": "./lib/tsdoc-metadata.json"
}
